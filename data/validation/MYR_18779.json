        "term": [
            {
                "speaker_id": "1",
                "transcription": "주거용이면서 임대용으로 쓰면 들어가요.",
                "sound_type": [
                    2
                ],
                "start": 0.000,
                "end": 3.563
            },
            {
                "speaker_id": "1",
                "transcription": "그러면 오피스텔 들어 갈까 안 들어갈까. 들어가죠.",
                "sound_type": [
                    2
                ],
                "start": 3.573,
                "end": 7.824
            },
            {
                "speaker_id": "1",
                "transcription": "원룸 들어 갈까 안 들어갈까. 들어가죠. 네.",
                "sound_type": [
                    2
                ],
                "start": 7.834,
                "end": 13.672
            },
            {
                "speaker_id": "1",
                "transcription": "자 그렇게 해 가지고 나온 결과가 이렇게 나옵니다.",
                "sound_type": [
                    2
                ],
                "start": 13.682,
                "end": 16.250
            },
            {
                "speaker_id": "1",
                "transcription": "아까 우리 잘 설명되어 있었죠.",
                "sound_type": [
                    2
                ],
                "start": 16.252,
                "end": 18.120
            },
            {
                "speaker_id": "1",
                "transcription": "그런데 표로 보니까 조금 더 여러분들이 눈에 들어오죠.",
                "sound_type": [
                    2
                ],
                "start": 18.120,
                "end": 20.412
            },
            {
                "speaker_id": "1",
                "transcription": "그래서 필요 경비가 등록했다 그러면은 60퍼센트.",
                "sound_type": [
                    2
                ],
                "start": 20.422,
                "end": 23.648
            },
            {
                "speaker_id": "1",
                "transcription": "이거 근데 분리 과세 시입니다. 분리 과세 시.",
                "sound_type": [
                    2
                ],
                "start": 23.658,
                "end": 25.791
            },
            {
                "speaker_id": "1",
                "transcription": "종합 과세되는 거 아니에요.",
                "sound_type": [
                    2
                ],
                "start": 25.801,
                "end": 27.180
            },
            {
                "speaker_id": "1",
                "transcription": "즉 2,000만 원 이하인 경우에 이렇게 된다.",
                "sound_type": [
                    2
                ],
                "start": 27.170,
                "end": 31.115
            },
            {
                "speaker_id": "1",
                "transcription": "그래서 등록인 경우에는 필요 경비 60, 미등록 50.",
                "sound_type": [
                    2
                ],
                "start": 31.125,
                "end": 34.440
            },
            {
                "speaker_id": "1",
                "transcription": "그리고 다른 종합 소득 금액이 2,000만 원 이하라면은 400만 원 기본 공제, 200만 원 기본 공제도 추가로 가능합니다.",
                "sound_type": [
                    2
                ],
                "start": 34.450,
                "end": 40.838
            },
            {
                "speaker_id": "1",
                "transcription": "그래서 계산해 보면.",
                "sound_type": [
                    2
                ],
                "start": 40.848,
                "end": 43.009
            }
        ]
