        "term": [
            {
                "speaker_id": "1",
                "transcription": "어 근데 왜 왜 이렇게 이런 걸 해줘요?",
                "sound_type": [
                    2
                ],
                "start": 0.000,
                "end": 1.768
            },
            {
                "speaker_id": "2",
                "transcription": "그러니까 미국에서는 죽음과 함께 세금이 사라진다라는 말이 있어요.",
                "sound_type": [
                    2
                ],
                "start": 1.778,
                "end": 6.004
            },
            {
                "speaker_id": "1",
                "transcription": "아니 죽음하고 세금은 피할 수 없다 그랬는데",
                "sound_type": [
                    2
                ],
                "start": 6.014,
                "end": 8.227
            },
            {
                "speaker_id": "2",
                "transcription": "아 한국하고 좀 다릅니다.",
