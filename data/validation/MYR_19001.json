            {
                "speaker_id": "1",
                "transcription": "뺀 후입니다.",
                "sound_type": [
                    2
                ],
                "start": 25.161,
                "end": 25.770
            },
            {
                "speaker_id": "2",
                "transcription": "자 뺀 후다.",
