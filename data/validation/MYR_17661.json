                    2
                ],
                "start": 63.938,
                "end": 64.721
            },
            {
                "speaker_id": "2",
                "transcription": "네. 지금도 미국에 있고 저는 한국에 들어와서",
                "sound_type": [
                    2
                ],
                "start": 64.731,
                "end": 67.501
            },
            {
                "speaker_id": "1",
                "transcription": "좀 왔다 갔다 하시는 거죠.",
                "sound_type": [
                    2
                ],
                "start": 67.511,
                "end": 68.862
            },
            {
                "speaker_id": "2",
                "transcription": "네 왔다 갔다 하고 한국 군대도 갔다 왔습니다.",
